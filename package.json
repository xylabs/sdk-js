{
  "license": "LGPL-3.0",
  "name": "@xylabs/sdk-js",
  "author": {
    "email": "support@xylabs.com",
    "name": "XY Labs Development Team",
    "url": "https://xylabs.com"
  },
  "bugs": {
    "email": "support@xylabs.com",
    "url": "https://github.com/xylabs/sdk-js/issues"
  },
  "workspaces": [
    "packages/**/*"
  ],
  "description": "Base functionality used throughout XY Labs TypeScript/JavaScript libraries",
  "docs": "dist/docs.json",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./dist/docs.json": {
      "default": "./dist/docs.json"
    },
    "./cjs": {
      "default": "./dist/index.js"
    },
    "./docs": {
      "default": "./dist/docs.json"
    },
    "./esm": {
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "homepage": "https://xylabs.com",
  "keywords": [
    "xylabs",
    "utility",
    "typescript",
    "esm"
  ],
  "dependencies": {
    "@xylabs/api": "workspace:~",
    "@xylabs/assert": "workspace:~",
    "@xylabs/bignumber": "workspace:~",
    "@xylabs/delay": "workspace:~",
    "@xylabs/eth-address": "workspace:~",
    "@xylabs/exists": "workspace:~",
    "@xylabs/forget": "workspace:~",
    "@xylabs/log": "workspace:~"
  },
  "devDependencies": {
    "@babel/core": "^7.22.15",
    "@babel/preset-env": "^7.22.15",
    "@swc/core": "^1.3.83",
    "@types/jest": "^29.5.4",
    "@types/mocha": "^10.0.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "@xylabs/eslint-config": "^2.19.5",
    "@xylabs/ts-scripts-yarn3": "^2.19.5",
    "@xylabs/tsconfig": "^2.19.5",
    "@xylabs/tsconfig-jest": "^2.19.5",
    "babel-jest": "^29.6.4",
    "depcheck": "^1.4.5",
    "eslint": "^8.48.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-deprecation": "^1.5.0",
    "eslint-plugin-import": "^2.28.1",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "jest-extended": "^4.0.1",
    "jest-sorted": "^1.0.14",
    "publint": "^0.2.2",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xylabs/sdk-js.git"
  },
  "resolutions": {
    "uuid": "^8"
  },
  "scripts": {
    "package-compile": "tsup && publint",
    "package-recompile": "tsup && publint"
  },
  "sideEffects": false,
  "version": "2.10.0",
  "packageManager": "yarn@3.6.3"
}