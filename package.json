{
  "name": "@xylabs/sdk-js",
  "version": "5.0.18",
  "description": "Base functionality used throughout XY Labs TypeScript/JavaScript libraries",
  "keywords": [
    "xylabs",
    "utility",
    "typescript",
    "esm"
  ],
  "homepage": "https://xylabs.com",
  "bugs": {
    "url": "git+https://github.com/xylabs/sdk-js/issues",
    "email": "support@xylabs.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xylabs/sdk-js.git"
  },
  "license": "LGPL-3.0-only",
  "author": {
    "name": "XY Labs Development Team",
    "email": "support@xylabs.com",
    "url": "https://xylabs.com"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/neutral/index.d.ts",
      "source": "./src/index.ts",
      "default": "./dist/neutral/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "module": "./dist/neutral/index.mjs",
  "source": "./src/index.ts",
  "types": "./dist/neutral/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "workspaces": [
    "packages/**/*"
  ],
  "scripts": {
    "build-typedoc-site": "xy gen-docs @xylabs/sdk-js",
    "readme:gen": "node scripts/readme-gen.mjs",
    "test": "vitest run"
  },
  "dependencies": {
    "@xylabs/api": "workspace:~",
    "@xylabs/array": "workspace:~",
    "@xylabs/assert": "workspace:~",
    "@xylabs/axios": "workspace:~",
    "@xylabs/base": "workspace:~",
    "@xylabs/bignumber": "workspace:~",
    "@xylabs/buffer": "workspace:~",
    "@xylabs/creatable": "workspace:~",
    "@xylabs/decimal-precision": "workspace:~",
    "@xylabs/delay": "workspace:~",
    "@xylabs/enum": "workspace:~",
    "@xylabs/error": "workspace:~",
    "@xylabs/eth-address": "workspace:~",
    "@xylabs/events": "workspace:~",
    "@xylabs/exists": "workspace:~",
    "@xylabs/forget": "workspace:~",
    "@xylabs/function-name": "workspace:~",
    "@xylabs/hex": "workspace:~",
    "@xylabs/log": "workspace:~",
    "@xylabs/logger": "workspace:~",
    "@xylabs/object": "workspace:~",
    "@xylabs/platform": "workspace:~",
    "@xylabs/profile": "workspace:~",
    "@xylabs/promise": "workspace:~",
    "@xylabs/retry": "workspace:~",
    "@xylabs/set": "workspace:~",
    "@xylabs/static-implements": "workspace:~",
    "@xylabs/storage": "workspace:~",
    "@xylabs/telemetry": "workspace:~",
    "@xylabs/telemetry-exporter": "workspace:~",
    "@xylabs/timer": "workspace:~",
    "@xylabs/typeof": "workspace:~",
    "@xylabs/url": "workspace:~"
  },
  "devDependencies": {
    "@types/node": "~24.10.0",
    "@typescript-eslint/eslint-plugin": "~8.46.3",
    "@typescript-eslint/parser": "~8.46.3",
    "@typescript-eslint/types": "~8.46.3",
    "@xylabs/eslint-config-flat": "~7.2.2",
    "@xylabs/ts-scripts-yarn3": "~7.2.2",
    "@xylabs/tsconfig": "~7.2.2",
    "@xylabs/tsconfig-dom": "~7.2.2",
    "dotenv": "~17.2.3",
    "eslint": "~9.39.1",
    "eslint-import-resolver-typescript": "~4.4.4",
    "jsdom": "~27.1.0",
    "knip": "~5.67.1",
    "lodash.merge": "~4.6.2",
    "typedoc": "~0.28.14",
    "typedoc-plugin-markdown": "~4.9.0",
    "typescript": "~5.9.3",
    "vitest": "~4.0.7"
  },
  "packageManager": "yarn@4.10.3",
  "engines": {
    "node": ">=18.17.1"
  },
  "engineStrict": true,
  "volta": {
    "node": "22.3.0",
    "yarn": "1.22.19"
  },
