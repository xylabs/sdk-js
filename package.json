    "@typescript-eslint/parser": "^8.3.0",
    "@xylabs/config": "^4.0.7",
    "@xylabs/eslint-config-flat": "^4.0.7",
    "@xylabs/ts-scripts-yarn3": "^4.0.7",
    "@xylabs/tsconfig": "^4.0.7",
    "@xylabs/tsconfig-jest": "^4.0.7",
    "dotenv": "^16.4.5",
    "eslint": "^9.9.1",
