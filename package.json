    "@typescript-eslint/parser": "^8.12.2",
    "@xylabs/config": "^4.2.3",
    "@xylabs/eslint-config-flat": "^4.2.3",
    "@xylabs/ts-scripts-yarn3": "^4.2.3",
    "@xylabs/tsconfig": "^4.2.3",
    "dotenv": "^16.4.5",
    "eslint": "^9.13.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "knip": "^5.34.4",
    "reflect-metadata": "^0.2.2",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.0",
