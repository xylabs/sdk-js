{
  "name": "@xylabs/threads-test",
  "version": "4.7.3",
  "private": true,
  "description": "Web workers & worker threads as simple as a function call",
  "keywords": [
    "thread",
    "worker",
    "pool",
    "spawn",
    "isomorphic",
    "parallel",
    "observable",
    "worker_threads"
  ],
  "homepage": "https://threads.js.org",
  "bugs": {
    "url": "git+https://github.com/andywer/threads.js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andywer/threads.js.git"
  },
  "funding": "https://github.com/andywer/threads.js?sponsor=1",
  "license": "MIT",
  "author": "Andy Wermke (https://github.com/andywer)",
  "sideEffects": [
    "./dist*/master/register.js",
    "./dist*/worker/index.js",
    "./register.*js",
    "./worker.*js"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/neutral/index.mjs",
      "default": "./dist/neutral/index.mjs"
    }
  },
  "main": "dist/neutral/index.mjs",
  "module": "dist/neutral/index.mjs",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/**",
    "*.js",
    "*.mjs",
    "*.ts"
  ],
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "./test/**/*.test.ts",
      "./test-tooling/**/*.test.ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "serial": true
  },
  "dependencies": {
    "@xylabs/threads": "workspace:^",
    "observable-fns": "^0.6.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/node": "^22.13.10",
    "@xylabs/eslint-config-flat": "^6.1.3",
    "@xylabs/ts-scripts-yarn3": "^6.1.3",
    "typescript": "^5.8.2",
    "vitest": "^3.0.9"
  },
  "optionalDependencies": {
    "tiny-worker": "^2.3.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
