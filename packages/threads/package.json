{
  "name": "@xylabs/threads",
  "version": "4.6.4",
  "description": "Web workers & worker threads as simple as a function call",
  "keywords": [
    "thread",
    "worker",
    "pool",
    "spawn",
    "isomorphic",
    "parallel",
    "observable",
    "worker_threads"
  ],
  "homepage": "https://threads.js.org",
  "bugs": {
    "url": "git+https://github.com/andywer/threads.js/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andywer/threads.js.git"
  },
  "funding": "https://github.com/andywer/threads.js?sponsor=1",
  "license": "MIT",
  "author": "Andy Wermke (https://github.com/andywer)",
  "sideEffects": [
    "./dist*/master/register.js",
    "./dist*/worker/index.js",
    "./register.*js",
    "./worker.*js"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "default": "./dist/esm/index.js"
    },
    "./implementation": {
      "browser": {
        "types": "./dist/types/master.d.ts",
        "import": "./dist/esm/master/implementation.browser.js",
        "default": "./dist/esm/master/implementation.browser.js"
      },
      "node": {
        "types": "./dist/types/master.d.ts",
        "import": "./dist/esm/master/implementation.node.js",
        "default": "./dist/esm/master/implementation.node.js"
      },
      "types": "./dist/types/master.d.ts",
      "import": "./dist/esm/master/implementation.js",
      "default": "./dist/esm/master/implementation.js"
    },
    "./master": {
      "types": "./dist/types/master.d.ts",
      "import": "./dist/esm/master/index.js",
      "default": "./dist/esm/master/index.js"
    },
    "./messenger": {
      "types": "./dist/types/messages.d.ts"
    },
    "./observable": {
      "types": "./dist/observable.d.ts",
      "require": "./dist/observable.js",
      "import": "./dist/esm/observable.js",
      "default": "./dist/esm/observable.js"
    },
    "./register": {
      "types": "./dist/master/register.d.ts",
      "import": "./dist/esm/master/register.js",
      "default": "./dist/esm/master/register.js"
    },
    "./worker": {
      "types": "./dist/types/worker.d.ts",
      "require": "./dist/worker/index.js",
      "import": "./dist/esm/worker/index.js",
      "default": "./dist/esm/worker/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**",
    "*.js",
    "*.mjs",
    "*.ts"
  ],
  "ava": {
    "extensions": [
      "ts"
    ],
    "files": [
      "./test/**/*.test.ts",
      "./test-tooling/**/*.test.ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "serial": true
  },
  "dependencies": {
    "callsites-3-1-0": "npm:callsites@3.1.0",
    "debug": "^4.4.0",
    "is-observable-2-1-0": "npm:is-observable@2.1.0",
    "observable-fns": "^0.6.1"
  },
  "devDependencies": {
    "@babel/types": "^7.26.10",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@types/debug": "^4.1.12",
    "@types/node": "^22.13.10",
    "@xylabs/eslint-config-flat": "^6.1.3",
    "@xylabs/ts-scripts-yarn3": "^6.1.3",
    "cross-env": "^7.0.3",
    "puppet-run": "^0.11.4",
    "raw-loader": "^4.0.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.36.0",
    "threads-plugin": "^1.4.0",
    "tiny-worker": "^2.3.0",
    "tslib": "^2.8.1",
    "typescript": "^5.8.2",
    "vitest": "^3.0.9",
    "webpack": "^5.98.0"
  },
  "optionalDependencies": {
    "tiny-worker": "^2.3.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
