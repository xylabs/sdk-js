  "name": "@xylabs/url",
  "author": {
    "email": "support@xylabs.com",
    "name": "XY Labs Development Team",
    "url": "https://xylabs.com"
  },
  "bugs": {
    "email": "support@xylabs.com",
    "url": "https://github.com/xylabs/sdk-js/issues"
  },
  "description": "Base functionality used throughout XY Labs TypeScript/JavaScript libraries",
  "docs": "dist/docs.json",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "browser": {
          "types": "./dist/browserIndex.d.ts",
          "default": "./dist/browserIndex.js"
        },
        "node": {
          "types": "./dist/nodeIndex.d.ts",
          "default": "./dist/nodeIndex.js"
        },
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "import": {
        "browser": {
          "types": "./dist/browserIndex.d.mts",
          "default": "./dist/browserIndex.mjs"
        },
        "node": {
          "types": "./dist/nodeIndex.d.mts",
          "default": "./dist/nodeIndex.mjs"
        },
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    },
    "./dist/docs.json": {
      "default": "./dist/docs.json"
    },
    "./docs": {
      "default": "./dist/docs.json"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "homepage": "https://xylabs.com",
  "keywords": [
    "xylabs",
    "utility",
    "typescript",
    "esm"
  ],
  "dependencies": {
    "buffer": "^6.0.3"
  },
  "devDependencies": {
    "@xylabs/ts-scripts-yarn3": "^3.0.0",
    "@xylabs/tsconfig": "^3.0.0",
    "@xylabs/tsconfig-dom": "^3.0.0",
    "typescript": "^5.2.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xylabs/sdk-js.git"
  },
  "sideEffects": false,
  "version": "2.11.3"
}
